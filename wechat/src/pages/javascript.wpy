<style lang="less">
</style>


<template>
<view>
  <text>{{container}}</text>
</view>
</template>



<script>
import wepy from 'wepy'

export default class Index extends wepy.page {
  config = {
    navigationBarTitleText: 'JavaScipt'
  }
  data = {
    container: ''
  }
  onLoad() {
    let self = this

    try {
      let article = wepy.getStorageSync('article')

      self.container = article
    } catch (err) {
      console.log(err)
    }
  }
}
</script>
